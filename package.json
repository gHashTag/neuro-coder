{
  "name": "grammy-vercel-boilerplate",
  "version": "1.0.0",
  "description": "Grammy boilerplate to develop and host telegram bots on Vercel",
  "main": "api/index.ts",
  "scripts": {
    "start": "node dist/api/index.js",
    "build": "tsc",
    "dev": "nodemon -r tsconfig-paths/register src/index.ts",
    "lint": "eslint . --cache --ext .ts,.js",
    "dev:server": "nodemon src/server.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neumanf/grammy-vercel-boilerplate.git"
  },
  "keywords": [
    "boilerplate",
    "telegram-bot",
    "vercel",
    "vercel-now",
    "grammy"
  ],
  "author": "neumanf",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/neumanf/grammy-vercel-boilerplate/issues"
  },
  "homepage": "https://github.com/neumanf/grammy-vercel-boilerplate#readme",
  "dependencies": {
    "@fal-ai/serverless-client": "^0.14.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffmpeg/ffmpeg": "^0.12.10",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@gradio/client": "^1.8.0",
    "@grammyjs/auto-retry": "^2.0.2",
    "@grammyjs/chat-members": "^1.1.0",
    "@grammyjs/conversations": "^1.2.0",
    "@grammyjs/files": "^1.1.1",
    "@grammyjs/menu": "^1.3.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/storage-free": "^2.4.2",
    "@grammyjs/types": "^3.17.0",
    "@runwayml/sdk": "^1.3.0",
    "@supabase/supabase-js": "^2.45.2",
    "@types/archiver": "^6.0.3",
    "archiver": "^7.0.1",
    "axios": "^1.7.7",
    "creatomate": "^1.2.1",
    "elevenlabs": "^0.16.1",
    "eslint-import-resolver-typescript": "^3.7.0",
    "express": "^4.21.0",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.1",
    "grammy": "^1.33.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.55.1",
    "pexels": "^1.4.0",
    "replicate": "^0.32.0",
    "sharp": "^0.33.4",
    "slideshow-video": "^1.0.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/node": "^17.0.5",
    "@types/node-fetch": "^2.6.11",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.8.1",
    "dotenv": "^10.0.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.8",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.5.4"
  }
}
